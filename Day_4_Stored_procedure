SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
ALTER Procedure [dbo].[uspProductList]
as
begin 
select 
product_name,list_price
from production.products
order by product_name;
end;


-- > to drop procedure we use

drop proc uspProductList;


